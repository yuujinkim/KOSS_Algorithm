import sys
from typing import List

N, M = map(int, input().split())
T: List[int] = [int(sys.stdin.readline()) for _ in range(N)]

start = 0
end = min(T)*M

while start <= end:
    total = 0
    mid = (start+end)//2
    for i in T:
        total += mid//i

    if total >= M:
        answer = mid
        end = mid-1
    else:
        start = mid+1

print(answer)
